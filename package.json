{
  "name": "native-animo",
  "version": "1.0.0",
  "description": "NativeAnimo is an app designed for React Native developers who want to add engaging and dynamic animations to their app interfaces. With NativeAnimo, users can access a vast library of animation source code and documentation, as well as experiment with a range of pre-built animation examples.",
  "private": true,
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "android": "yarn workspace mobile android",
    "android:bundle": "yarn workspace mobile android:bundle",
    "android:apk": "yarn workspace mobile android:apk",
    "android:clean": "yarn workspace mobile android:clean",
    "commit": "git-cz",
    "prettier": "prettier --write \"**/*.{js,json,md,yml}\"",
    "format": "prettier --write .",
    "start": "yarn workspace mobile start  --reset-cache",
    "test": "echo \"Error: no test specified\" && exit 1",
    "web": "yarn workspace web start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yokesh-ks/NativeAnimo.git"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "commitizen": "^4.3.0",
    "customize-cra": "^1.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.71.3",
    "react-native-web": "^0.18.12",
    "react-native-webview": "^11.26.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-react-native-web": "^0.18.12",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-app-rewired": "^2.2.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yokesh-ks/NativeAnimo/issues"
  },
  "homepage": "https://github.com/yokesh-ks/NativeAnimo#readme"
}
